{{#each this}}
<div class="photo-card">
    <img src="{{url}}" alt="{{tags}}" loading="lazy" width="400">
    <div class="info">
        <p class="info-item">
            <b>{{likes}}</b>
        </p>
        <p class="info-item">
            <b>{{views}}</b>
        </p>
        <p class="info-item">
            <b>{{comments}}</b>
        </p>
        <p class="info-item">
            <b>{{downloads}<}/b>
        </p>
    </div>
</div>
{{/each}}